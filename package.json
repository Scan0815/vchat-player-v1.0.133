{
    "name": "vchat-player",
    "version": "1.0.133",
    "description": "vchat-player",
    "directories": {
        "dist": "./dist"
    },
    "main": "./dist/vchat-player.js",
    "types": "./dist/vchat-player.d.ts",
    "private": false,
    "contributors": [
        {
            "name": "Special Projects Team",
            "email": "special-projects-squad@campoint.net"
        }
    ],
    "license": "MIT",
    "dependencies": {
        "detectrtc": "^1.3.9",
        "hls.js": "0.13.1",
        "image-capture": "^0.4.0",
        "platform": "^1.3.5",
        "ts-polyfill": "3.6.5",
        "vchat-core": "^5.1.0"
    },
    "devDependencies": {
        "@types/hls.js": "^0.12.5",
        "@types/jest": "^25.1.3",
        "@types/platform": "^1.3.2",
        "@types/w3c-image-capture": "^1.0.2",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "clean-webpack-plugin": "^3.0.0",
        "eslint": "^6.8.0",
        "eslint-plugin-jest": "^23.6.0",
        "eslint-plugin-sonarjs": "^0.5.0",
        "eslint-plugin-tsdoc": "^0.2.1",
        "eslint-plugin-unicorn": "^16.0.0",
        "eslint-teamcity": "^2.2.0",
        "husky": "^4.2.1",
        "jest": "^25.1.0",
        "jest-teamcity": "^1.6.0",
        "prettier": "1.19.1",
        "pretty-quick": "^2.0.1",
        "puppeteer": "^2.1.1",
        "rimraf": "^3.0.2",
        "source-map-loader": "^0.2.4",
        "string-replace-loader": "^2.1.1",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.0",
        "ts-mockito": "^2.5.0",
        "typedoc": "^0.16.10",
        "typescript": "3.6.5",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11",
        "webpack-merge": "^4.2.1"
    },
    "scripts": {
        "clean": "rimraf ./dist",
        "build": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "watch": "webpack --config webpack.dev.js --watch",
        "test": "jest",
        "test:teamcity": "yarn test",
        "lint": "eslint src/**/*.ts",
        "lint:teamcity": "yarn lint --format ./node_modules/eslint-teamcity/index.js",
        "style": "prettier --write \"{src,test}/**/*.ts\"",
        "generate:docs": "typedoc --mode file -target ES6 --out ./docs ./src"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "prettier": {
        "singleQuote": true,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testResultsProcessor": "jest-teamcity"
    }
}
